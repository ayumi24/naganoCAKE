<h1>商品新規登録</h1>
  <%= form_with model: @item, url: admin_items_path, method: :post do |f| %>

  <h4>商品画像</h4>
    <%= f.file_field :image, accept: "image/*" %>
  <h4>商品名</h4>
    <%=  f.text_field :name %>
  <h4>商品説明</h4>
    <%= f.text_area :introduction %>
  <h4>税抜価格</h4>
  　<%= f.text_field :price %>
  <h4>ジャンル</h4>
    <%= f.select :genre_id,[['ケーキ',1],['プリン',2],['焼き菓子',3],['キャンディ',4]] %><br>
  <%= f.submit '新規登録', class: "btn btn-sm btn-success" %>

<% end %>