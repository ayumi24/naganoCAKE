<h2>会員一覧</h2>
<table class="table table-striped">
  <tbody>
    <% @customers.each do |c| %>
    <tr>
      <td><%= c.id %></td>
      <td><%= link_to  admin_customer_path(c.id) do %>
      <%= c.last_name %><%= c.first_name %>
      <% end %></td>
      <td><%= c.email %></td>
    </tr>
    <% end %>
  </tbody>
</table>